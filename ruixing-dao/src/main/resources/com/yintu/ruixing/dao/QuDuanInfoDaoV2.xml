    <select id="findDianMaHuaDatabById" resultMap="quduanBaseResultMap" parameterType="java.lang.Integer">
        SELECT qb. typeid ,
               qi.Design_carrier,
               qi.v3 V_out_zhu,
               qi.v9 V_out_bei,
               qi.v4 mA_out_zhu,
               qi.v10 mA_out_bei,
               qi.v5 Hz_up_zhu,
               qi.v11 Hz_up_bei,
               qi.v6 Hz_down_zhu,
               qi.v12 Hz_down_bei,
               qi.v7 Hz_low_zhu,
               qi.v13 Hz_low_bei,
               qi.v8 FBJ_drive_zhu,
               qi.v14 FBJ_drive_bei,
               qi.FBJ_collection_zhu,
               qi.FBJ_collection_bei,
               sb.sb_name
        FROM data_applydata qi,
             mro_shebei sb,
             mro_quduan_base qb
        WHERE sb.id = qb.sid
          AND qi.v1 = qb.qdid
          AND qi.v1 = #{id}
        GROUP BY sb.sb_name
    </select>


    <!--自定义查询-->
    <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from data_applydata qi inner join mro_quduan_base qb on
        qi.v1 = qb.qdid and qi.station_id = qb.cid
        where qi.id = #{id,jdbcType=INTEGER}
    </select>

    <!--按照车站查询一条详情-->
    <select id="selectLastByCid" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from data_applydata qi inner join mro_quduan_base qb on
        qi.v1 = qb.qdid and qi.station_id = qb.cid
        <where>
            <if test="cid!=null">
                and qi.station_id=#{cid,jdbcType=INTEGER}
            </if>
        </where>
        order by qi.id desc limit 1
    </select>

    <!--按照车站和区段id，时间查询一条区段详情-->
    <select id="selectByCidAndQdIdAndTime" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from data_applydata qi inner join mro_quduan_base qb on
        qi.v1 = qb.qdid and qi.station_id = qb.cid
        <where>
            <if test="cid!=null">
                and qi.station_id=#{cid,jdbcType=INTEGER}
            </if>
            <if test="qdId!=null">
                and qi.v1=#{qdId,jdbcType=INTEGER}
            </if>
            <if test="time!=null">
                and qi.createtime=UNIX_TIMESTAMP(#{time,jdbcType=TIMESTAMP})
            </if>
        </where>
    </select>

    <!--数据下载-->
    <select id="selectByXidAndCidAndBetweenAndTime" resultType="Integer">
        select
        qi.id
        from data_applydata qi inner join mro_quduan_base qb on
        qi.v1 = qb.qdid and qi.station_id = qb.cid
        <where>
            <if test="cid!=null">
                and qb.cid=#{cid,jdbcType=INTEGER}
            </if>
            <if test="startTime!=null and endTime!=null">
                and qi.createtime between UNIX_TIMESTAMP(#{startTime,jdbcType=TIMESTAMP}) and UNIX_TIMESTAMP(#{endTime,jdbcType=TIMESTAMP})
            </if>
        </where>
    </select>

