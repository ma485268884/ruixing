    <!--根据传来的时间段和区段名和属性名 按秒来 查询对应的数据   -->
    <select id="findQuDuanData" resultType="java.lang.Integer" statementType="STATEMENT">
        select ${shuxingname}
        from data_applydata qi,
             mro_quduan_base qb
        where qi.v1 = qb.qdid
          and qb.quDuanYunYing_name = "${quduanname}"
          and DATE_FORMAT(qi.time, "%Y/%m/%d %H:%i:%S")
            BETWEEN
            DATE_FORMAT("${starttime}", "%Y/%m/%d %H:%i:%S")
            and
            DATE_FORMAT("${endtime}", "%Y/%m/%d %H:%i:%S")
    </select>

